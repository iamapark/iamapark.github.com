<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE sqlMap PUBLIC "-//iBATIS.com//DTD SQL Map 2.0//EN" "http://www.ibatis.com/dtd/sql-map-
2.dtd">

<sqlMap namespace="Login">

	<resultMap id="LoginList" class="Login">
		<result property="userId" column="admin_id" />
		<result property="password" column="password" />
	</resultMap>
	
	<select id="adminLoginCheck" parameterClass="Login" resultClass="Login">
		select * from admin 
		where admin_id=#userId# and password=#password#
	</select>
	
</sqlMap>